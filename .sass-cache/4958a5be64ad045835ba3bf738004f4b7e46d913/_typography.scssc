/* Global */
html {
	font-size:18px;
	height:100%;
	-webkit-text-size-adjust:100%; 
	    -ms-text-size-adjust:100%; 
		 	text-size-adjust:100%;
	-webkit-text-rendering:optimizeLegibility;
	   -moz-text-rendering:optimizeLegibility;
			text-rendering:optimizeLegibility;
}

@media only screen and (min-width: 0px) and (max-width: 760px) { 
	html {
		font-size:16px;
	}
}

body {
	color:#333;
	font: 400 100%/24px "Proxima-Nova",Helvetica, Arial, Sans-Serif;
	height:100%; 
	word-wrap: break-word;
	-webkit-font-smoothing: antialiased; 
			font-smoothing: antialiased;
	-webkit-hyphens: auto; 
	   -moz-hyphens: auto; 
	    -ms-hyphens: auto; 
			hyphens: auto; 
}

@media only screen and (min-width: 0px) and (max-width: 1000px) { 
	body {	
		-webkit-touch-callout: none;  
		  -webkit-user-select: none;  
		   -khtml-user-select: none;  
		     -moz-user-select: none;  
		      -ms-user-select: none;  
		          user-select: none;
	}
}

@media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 192dpi) { 
    /* Retina-specific body weights & smoothing */
	body {
		-webkit-font-smoothing: antialiased; 
				font-smoothing: antialiased;
	}
}

/* Mozilla based browsers */
::-moz-selection {
       background-color: #EEE;
       color: #222;
}

/* Works in Safari */
::selection {
       background-color: #EEE;
       color: #222;
}

/* Headlines */
h1, h2, h3, h4, h5 { font-weight:700; text-indent:-2px; }
h1 { font-size:48px; line-height:48px; letter-spacing:-0.010em; }
h2 { font-size:30px; line-height:30px; letter-spacing:-0.020em; }
h3 { font-size:24px; line-height:21px; letter-spacing:-0.020em; }
h4 { font-size:18px; line-height:18px; letter-spacing:-0.020em; }
h5 { font-size:16px; line-height:18px; letter-spacing:-0.020em; }

@media only screen and (min-width: 0px) and (max-width: 760px) {
	h1 { font-size:36px; line-height:36px; letter-spacing:-0.010em; }
	h2 { font-size:24px; line-height:24px; letter-spacing:-0.020em; }
	h3 { font-size:18px; line-height:18px; letter-spacing:-0.020em; }
	h4 { font-size:16px; line-height:16px; letter-spacing:-0.020em; }
	h5 { font-size:14px; line-height:14px; letter-spacing:-0.020em; }
}

.semi { font-weight:600; }
.normal { font-weight:300; }
.light { font-weight:100; }

/* Hyperlinks */

a { 
	color: #397ca0; 
	-webkit-tap-highlight-color: rgba(0,0,0,0); 
 	-webkit-transition: color 0.3s ease;
	   -moz-transition: color 0.3s ease;	
			transition: color 0.3s ease;
}
	a:hover { color: #397CA0; }
	
	a[href^="http://"]:not([href*="localhost:8888"])::after {
		background:url(data:image/png;base64,);
		background-size:10px 10px;
		content: "";
		display:inline-block;
		width:10px;
		height:10px;
		margin:0 3px;
	}


/* Paragraphs */

p { 
	hyphenate: auto;
	hyphenate-lines: 3;
	font-size:1rem; 
	margin:24px 0;
	letter-spacing:-0.020em;
	word-spacing: 0.025em;
	word-wrap:normal;
}

p:before, p:after { 
	content: ""; 
	display: block; 
	height: 0px; 
	margin: 0px;
}

p a {
    margin: -.5em;
    padding: .5em;
    position: relative;
}

p.lead {
	font-size:30px;
	font-weight:200;
	letter-spacing:-0.020em;
	line-height:36px;
	margin:36px 0;
}

@media only screen and (min-width: 0px) and (max-width: 760px) { 
	p.lead {
		font-size:18px;
		line-height:24px;
		margin:24px 0;
	}
}

.hint {
	font-size:16px;
	line-height:24px;
	font-weight:300;
	-webkit-font-smoothing: subpixel-antialiased; 
			font-smoothing: subpixel-antialiased;
}

@media only screen and (min-width: 0px) and (max-width: 760px) { 
	.hint {
		font-size:12px;
	}
}

b, strong, caption, th, thead, dt, legend { 
	font-weight:700;
}

cite, dfn, em, i { 
	font-style:italic;
}

code, kbd, samp, pre, tt, var { 
	font-family:mono-space, monospace;
	font-size:16px;
	line-height:24px;
}

del { 
	text-decoration:line-through;
}

ins	{ 
	color: #f00;
}

sup, sub { 
	font-size:80%; 
	line-height:0;
}

sup { 
	vertical-align:super;
}
sub {
	vertical-align:sub; 
}

abbr, abbr[title], acronym[title], dfn[title] { 
	border-bottom:dotted 1px #ccc; 
	cursor:pointer;
}

code {
	background:#f6f6f6;
	border:solid 1px #ededed;
	color:#B8A45F;
	display:inline-block; 
	font-size:14px;
	line-height:16px;
	margin:-5px 0;
	padding:0 3px;
	-webkit-border-radius:3px;
	   -moz-border-radius:3px;
			border-radius:3px;
}

blockquote { 
	color:#222;
	font-style:italic;
	font-size:20px;
	line-height:24px;
	margin:30px 0;
	padding:2px 0 2px 2.0em;
	position:relative;
	quotes: "\201C" "\201D";
}

blockquote:before { 
	color:#222;
	content: open-quote; 
	font: italic 100%/60% 'Playfair Display', Georgia, Serif;
	font-size:72px;
	font-style:normal;
	left:0px;
	position:absolute;
	width:0px;
} 

address {
	line-height:20px;
	margin:20px 0;
}
address a,
address span { display:block; }

/* Lists */
/* Add classes so that this does not automatically apply, since lists are sometimes used for layout */
ul, ol { 
	margin:20px 0 20px 1em;
}

ol ol,
ul ul,
ol ul,
ul ol { margin: 0 0 0 1.3em; }

ul li {
	list-style: disc outside;
}

ol li {
	list-style: decimal outside;
}

dt, dl { line-height:18px; }
dt { margin-top:10px; }

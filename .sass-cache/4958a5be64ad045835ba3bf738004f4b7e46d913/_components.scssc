@linei:@template"G#body.shifted article {  
	top:-100%;
}

/* Article Body */


/* Article Card Component */
.article-card {
	background:#fff;
	box-shadow:0 0 10px rgba(0,0,0,0.2);
	color:#222;
	display:block;
	margin:20px 0;
	overflow:hidden;
}

.article-card-top {
	position:relative;
}

.article-card-title {
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  rgba(0,0,0,0) 0%, rgba(0,0,0,0.65) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,0)), color-stop(100%,rgba(0,0,0,0.65))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.65) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.65) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.65) 100%); /* IE10+ */
	background: linear-gradient(to bottom,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.65) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00000000', endColorstr='#a6000000',GradientType=0 ); /* IE6-8 */
	bottom:0px;
	color:#fff;
	left:0px;
	padding:40px 0;
	position:absolute;
	right:0px;
}

.article-card-title h2,
.article-card-title h4 {
	border-left:solid 5px #fff;
	padding:0 80px;
}

.article-card-bottom {
	padding:20px 80px;
}

@media only screen and (min-width: 0px) and (max-width: 760px) {
	
	.article-card {
		margin:10px 0 20px 0;
	}
	
	.article-card-title {
		padding:20px 0;
	}
	
	.article-card-title h2,
	.article-card-title h4 {
		border-left:solid 3px #fff;
		padding:0 20px;
	}
	 
	.article-card-bottom {
		padding:0 20px;
	}
}

.article-card-bottom span {
	display:block;
}

.article-pagination {
	text-align:center;
	padding:50px 0;
}

/* Comment Component */

.article-comments {
	margin:40px 0;
}
.article-comments form {
	border:solid 2px #efefef;
	-webkit-border-radius:3px;
	   -moz-border-radius:3px;
			border-radius:3px;
}

.article-comments form input[type="text"],
.article-comments form textarea {
	border:none;
	margin:0;
	padding:10px;
	-webkit-border-radius:0px;
	   -moz-border-radius:0px;
			border-radius:0px;
}

.article-comments form input[type="text"] {
	border-bottom:solid 2px #efefef;
}

.article-comments form input[type="submit"] {
	margin:0;
}

.article-comments label:last-child {
	background:#f6f6f6;
	padding:10px;
}

.article-comment {
	border-bottom:solid 1px #ededed;
	margin:10px 0;
	padding:10px 0;
}

.article-comment h5 {
	margin:0 0 0 2px;
}

/* Hamburger Menu Component */
.hamburger {
	background:#ddd;
	display:none;
	float:left;
	height:2px;
	margin:2px 10px 0 0;
	padding:0;
	width:16px;

	&:before {
		background:#ddd;
		content:"";
		display:inline-block;
		float:left;
		height:2px;
		margin:5px 10px 3px 0;
		padding:0;
		width:16px;	
	}
	
	&:after {
		background:#ddd;
		content:"";
		display:inline-block;
		float:left;
		height:2px;
		margin:0;
		padding:0;
		width:16px;	
	}
}



/* Progress Component */
.progress {
	background:#000;
	box-shadow:0 0 5px rgba(0,0,0,0.6);
	height:3px;
	margin:-3px 0 0 0;
	position:fixed;
	left:0px;
	top:3px;
	width:0%;
	z-index:99999;
	-webkit-transition: all 1.4s ease;
	   -moz-transition: all 1.4s ease;
			transition: all 1.4s ease;
}

/* Full Screen Background */
.fullscreen {
	background:#fff url(../img/layout/michael_stevens_articles_bg.jpg) no-repeat center top;
	bottom:0px;
	left:0px;
	position:fixed;
	right:0px;
	top:0px;
}

/* Success Message Component */
.success-msg {
	background:#397ca0;
	color:#fff;
	display:none;
	font-weight:600;
	margin:-1px -1px 10px -1px;
	padding:10px;
	-webkit-border-radius:3px 3px 0 0;
	   -moz-border-radius:3px 3px 0 0;
			border-radius:3px 3px 0 0;
}

/* Error Message Component */
.error-msg {
	background:#e74c3c;
	color:#fff;
	display:none;
	font-weight:600;
	margin:-1px -1px 10px -1px;
	padding:10px;
	-webkit-border-radius:3px 3px 0 0;
	   -moz-border-radius:3px 3px 0 0;
			border-radius:3px 3px 0 0;
}

.error-msg .icons {
	font-weight:500;
}

/* Article Detail Styles */
.article-detail {
	
	.article-head {
		background-color:#000;
		background-attachment:fixed;
		background-repeat:no-repeat;
		background-size:1600px auto;
		background-position:top center;
		color:#fff;
		margin:0 0 40px 0;
		overflow:hidden;
		padding:140px 0 20px 0;
		position:relative;
		text-align:center;
		width:100%;
		
		a {
			color:#fff;
		}
		
		.lead {
			margin:80px 0;
		}
		
		h1 {
			font-size:72px;
			line-height:60px;
			letter-spacing:-0.04em;
		}
		
		h2 {
			margin:8px 0;
		}
		
		h4 {
			margin:16px 0;
		}
		
		.timer {
			border:solid 1px #aaa;
			color:#aaa;
			display:inline-block;
			padding:5px 20px;
			position:absolute;
			bottom:20px;
			right:20px;
			-webkit-border-radius:50px;
			   -moz-border-radius:50px;
					border-radius:50px;
		}
	}
	
	.article-body {
		padding:40px 0;
		
		figcaption {
			background:#f6f6f6;
			color:#333;
			display:block;
			font-size:14px;
			padding:16px 20px;
		}
		
		.article-img-left {
			float:left;
			margin:8px 40px 40px 0;
			width:400px;
			
			&:after {
				content:attr(data-licence);
				display:block;
				font-size:8px;
				text-align:right;
				text-transform:uppercase;
			}
		}
		
		.article-img-right {
			float:right;
			margin:8px -200px 40px 40px;
			width:600px;
			
			&:after {
				content:attr(data-licence);
				display:block;
				font-size:8px;
				text-align:right;
				text-transform:uppercase;
			}
		}
		
		.article-img-fill {
			margin:0;
			
			&:after {
				content:attr(data-licence);
				display:block;
				font-size:8px;
				text-align:right;
				text-transform:uppercase;
			}
		}
		
		.article-img-overfill {
			margin:0 -80px;
			
			&:after {
				content:attr(data-licence);
				display:block;
				font-size:8px;
				text-align:right;
				text-transform:uppercase;
			}
		}
		
		.article-img-full {
			background-attachment:fixed;
			background-size:100% 100%;
			box-shadow:inset 0 0 40px rgba(0,0,0,0.6);
			height:700px;
			margin:80px 0px;
			overflow:hidden;
			position:relative;
			
			figcaption {
				background:rgba(255,255,255,0.8);
				border-top:solid 3px #fff;
				bottom:20px;
				position:absolute;
				right:20px;
			}
			
		}
		
		.article-img-left-full {
			margin:0;
			
			img {
				margin:20px 0 40px 0;
			}
			
			p {
				float:left;
				margin:20px 0 20px 40px;
				width:340px; 
			}
		}
		
		.article-img-right-full {
			margin:0;
			
			img {
				margin:20px 0 40px 0;
			}
			
			p {
				float:right;
				margin:20px 40px 20px 0;
				width:340px; 
			}
		}
		
		
	}
	
}

/* Pager Component */
.onepage { 
	overflow:hidden; 
	min-height:500px; 
	position:relative;
}
	
.onepage .slides {
	bottom:0px; 
	left:0px; 
	overflow:hidden;
	position:absolute; 
	right:0px; 
	top:0px;
}

.onepage .slides .slide-bg { 
	bottom:0px;
	left:0px; 
	overflow:hidden;
	position:absolute;
	right:0px; 
	top:0px;
	text-align:center;
	outline:solid 1px red;
}

.onepage .slides .stage {
	bottom:0px;
	color:#fff;
	left:0px;
	position:absolute;
	right:0px; 
	text-align:center;
	top:0px;
}

.onepage .slides canvas {
	position:absolute;
	left:50%;
	top:50%;
	margin:-300px 0 0 -300px;
}

@media only screen and (min-width: 0px) and (max-width: 700px) { 
	
	.onepage { 
		overflow:visible; 
		height:auto; 
		position:static;
	}
	
	.onepage .slides {
		position:static;
		height:480px;
		overflow:hidden;
	}
}

/* Spec Component */

.spec {
	margin:10px 0;
}
.spec dt {
	float:left;
	margin:0 20px 0 0;
	width:50%;
}

.spec dd {
	margin:0 0 20px 0;
	overflow:hidden;
}

/* Vertical Dot Component */	
.vertical-dots { 
	display:block; 
	position:absolute; 
	right:10px; 
	top:40%; 
	z-index:99999;
}

.vertical-dots li { 
	background:#fff; 
	color:#fff; 
	display:block; 
	height:7px; 
	margin:7px 0; 
	width:7px;
	-webkit-border-radius:7px;
	   -moz-border-radius:7px;
			border-radius:7px;
}

/* Prettify Component */
.pln{color:#e682a9}
@media screen{
	.str{color:pink}
	.kwd{color:#88cbf0}
	.com{color:#dbc673}
	.typ{color:pink}
	.lit{color:#88cbf0}
	.pun,.opn,.clo{color:#7e8998}
	.tag{color:pink}
	.atn{color:#8ba771}
	.atv{color:#88cbf0}
	.dec,.var{color:orange}
	.fun{color:red}
}
